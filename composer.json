{
    "name": "appletonlearning/laravel-postmark-channel",
    "description": "A Postmark channel for Laravel notifications.",
    "homepage": "https://github.com/appletonlearning/laravel-postmark-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Campbell",
            "email": "adam@spurjobs.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "wildbit/postmark-php": "^2.5",
        "illuminate/mail": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Spur\\Postmark\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spur\\Postmark\\PostmarkServiceProvider"
            ]
        }
    }
}
